{
  "rewrites": [
    {
      "source": "/api/checkout",
      "destination": "/api/checkout.js"
    },
    {
      "source": "/api/sales-log",
      "destination": "/api/sales-log.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}